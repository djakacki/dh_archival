         <group>
      
            <optional>
               <ref name="desc"/>
            </optional>
      
      
            <oneOrMore>
               <choice>
                  <ref name="char"/>
                  <ref name="glyph"/>
               </choice>
            </oneOrMore>
      
         </group>
         <ref name="att.global.attributes"/>
         <empty/>
      </element>
   </define>

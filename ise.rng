            <optional>
               <ref name="glyphName"/>
            </optional>
      
      
            <zeroOrMore>
               <ref name="model.descLike"/>
            </zeroOrMore>
      
      
            <zeroOrMore>
               <ref name="charProp"/>
            </zeroOrMore>
      
      
            <zeroOrMore>
               <ref name="mapping"/>
            </zeroOrMore>
      
      
            <zeroOrMore>
               <ref name="figure"/>
            </zeroOrMore>
      
      
            <zeroOrMore>
               <ref name="model.graphicLike"/>
            </zeroOrMore>
      
      
            <zeroOrMore>
               <ref name="model.noteLike"/>
            </zeroOrMore>
      
         </group>
         <ref name="att.global.attributes"/>
         <empty/>
      </element>
